<%- include('partials/head') -%>
<%- include('partials/header') -%>
<div class="table-wrapper">
<table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Shop</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
        <% for (var i = 0; i < shops.length; i++) { %>
        <tr>
            <td><%= shops[i].name %></td>
            <td>Address: <%= shops[i].address %></td>
            <td><a href="/shop/<%= shops[i]._id %>" class="btn btn-primary">View</a></td>
        </tr>
        <% } %>
    </tbody>
</table>
</div>
<%- include('partials/footer') -%>